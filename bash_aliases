# File Navigation
alias ll='ls -alF'
alias l='ls -CF'
alias la='ls -al'
alias ~='cd ~'
alias ..='cd ..'
alias ....='cd ../..'
alias dt="cd ~/Desktop"

# Git
alias gs="git status"
alias ga="git add ."
alias gd="git diff"
alias gp="git push"
alias gb="git branch"

# Sudo
alias svim='sudo vi'
alias snano='sudo nano'
